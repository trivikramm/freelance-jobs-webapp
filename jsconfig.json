{
  "compilerOptions": {
    "checkJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "jsx": "react-jsx",
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "baseUrl": "."
  },
  "include": ["src/**/*"],
  "exclude": ["dist", "node_modules", "Freelance-Jobs-WebApp", "AI-Powered Document Intelligence Platform"]
}